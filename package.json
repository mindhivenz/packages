  "scripts": {
    "clean-all": "rm -rf ./node_modules && rm -rf ./packages/*/node_modules && yarn run clean-build",
    "clean-build": "rm -rf ./packages/*/build",
    "bootstrap": "yarn run clean-all && yarn",
    "build": "lerna run build",
    "new": "node ./scripts/new-package.js",
    "postinstall": "lerna bootstrap && yarn run build --silent",
    "pub": "yarn run build-clean --silent && yarn run build --silent && lerna publish --access public --silent",
