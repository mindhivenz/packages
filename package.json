{
  "name": "mhp",
  "config": {
    "scripts_dest": "lib/scripts",
    "scripts_src": "src/scripts",
    "packages_out": "lib/packages"
  },
  "watch": {
    "build:scripts": {
      "patterns": [
        "src/scripts",
        "test"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "mhp:build": "node $npm_package_config_scripts_dest/build.js",
    "mhp:clean": "node $npm_package_config_scripts_dest/clean.js",
    "mhp:pub": "node $npm_package_config_scripts_dest/pub.js",
    "mhp:new": "node $npm_package_config_scripts_dest/new.js",
    "mhp:nested-install": "node $npm_package_config_scripts_dest/installNestedPackageDeps.js",
    "bootstrap": "yarn run clean:all && yarn",
    "clean:build": "yarn mhp:clean",
    "preclean:all": "yarn clean:build",
    "clean:all": "rm -rf ./node_modules & rm -rf ./src/packages/*/node_modules",
    "prebuild:scripts": "rm -rf -- $npm_package_config_scripts_dest",
    "build:scripts": "babel src/scripts  --out-dir $npm_package_config_scripts_dest",
    "build": "yarn mhp:build",
    "new": "yarn mhp:new",
    "pub": "yarn mhp:pub",
    "postinstall": "yarn build:scripts && yarn mhp:nested-install",
    "watch": "npm-watch"
  },
  "dependencies": {
    "@mindhive/components": "^4.5.35",
    "@mindhive/di": "^4.3.1",
    "@mindhive/forms": "^0.0.1-1",
